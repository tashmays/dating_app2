<div class="row">
	<div class="col s10 offset-s1">
		<h1>Hello <%= "#{User.find(@profile.user_id).user_name}" %></h1>
		<hr />
		<h4>Bio:</h4>
		<p style="font-size: 24px"><%= @profile.bio %></p>
		<br />
		<h4>Age:</h4>
		<p style="font-size: 24px"><%= @profile.age %></p>
		<br />
		<h4>Gender:</h4>
		<p style="font-size: 24px"><%= @profile.gender %></p>
		<br />
		<h4>City:</h4>
		<p style="font-size: 24px"><%= @profile.city %></p>
		<br />
		<%= link_to 'Home Page', root_path %> |
    <%= link_to 'Search', profile_index_path %> |
		<%= link_to 'Edit Profile', edit_profile_path(@profile) %>
	</div>
</div>
